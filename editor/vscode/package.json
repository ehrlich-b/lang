{
  "name": "language-lang",
  "displayName": "Language",
  "description": "Syntax highlighting for the 'language' programming language",
  "version": "0.0.1",
  "publisher": "language",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [
      {
        "id": "lang",
        "aliases": ["Language", "lang"],
        "extensions": [".lang"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "lang",
        "scopeName": "source.lang",
        "path": "./syntaxes/lang.tmLanguage.json"
      }
    ]
  }
}
