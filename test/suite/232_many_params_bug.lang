// expect: 46
// Test: 7+ parameters with result accumulation
// Regression test for 7+ param accumulator bug (now fixed)

func add8(a i64, b i64, c i64, d i64, e i64, f i64, g i64, h i64) i64 {
    return a + b + c + d + e + f + g + h;
}

func main() i64 {
    var result i64 = 0;
    result = result + add8(1, 2, 3, 4, 5, 6, 7, 8);  // 36
    result = result + 10;  // 46
    return result;  // 46
}
