// expect: 215
// Float: basic arithmetic, comparisons, and function parameters

func double_it(x f64) f64 {
    return x + x;
}

func add_floats(a f64, b f64) f64 {
    return a + b;
}

func main() i64 {
    var result i64 = 0;

    // fadd: 1.5 + 2.5 = 4.0
    if 1.5 + 2.5 > 3.9 {
        result = result + 10;
    }

    // fsub: 5.5 - 2.5 = 3.0
    if 5.5 - 2.5 > 2.9 {
        result = result + 20;
    }

    // fmul: 2.5 * 4.0 = 10.0
    if 2.5 * 4.0 > 9.9 {
        result = result + 30;
    }

    // fdiv: 10.0 / 4.0 = 2.5
    if 10.0 / 4.0 > 2.4 {
        result = result + 40;
    }

    // fcmp oeq (==)
    if 3.0 == 3.0 {
        result = result + 1;
    }

    // fcmp one (!=)
    if 3.0 != 4.0 {
        result = result + 2;
    }

    // fcmp olt (<)
    if 2.5 < 3.5 {
        result = result + 3;
    }

    // fcmp ole (<=)
    if 3.5 <= 3.5 {
        result = result + 4;
    }

    // fcmp oge (>=)
    if 4.5 >= 4.5 {
        result = result + 5;
    }

    // Float function parameters
    // double_it(2.5) = 5.0
    if double_it(2.5) > 4.9 {
        result = result + 50;
    }

    // add_floats(3.5, 4.5) = 8.0
    if add_floats(3.5, 4.5) > 7.9 {
        result = result + 50;
    }

    // All pass: 10+20+30+40+1+2+3+4+5+50+50 = 215
    return result;
}
